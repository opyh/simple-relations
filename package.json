{
  "name": "simple-relations",
  "version": "1.1.1",
  "description": "Define relations between MongoDB documents and get simple-to-use helpers.",
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/opyh/simple-relations.git"
  },
  "author": "Sebastian Felix Zappe",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/opyh/simple-relations/issues"
  },
  "homepage": "https://github.com/opyh/simple-relations#readme",
  "dependencies": {
    "inflection": "^1.12.0",
    "lodash.memoize": "^4.1.2",
    "underscore.string": "^3.3.4"
  },
  "devDependencies": {
    "ava": "^0.24.0",
    "babel-cli": "^6.26.0",
    "babel-core": "6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react-app": "^3.1.1",
    "babel-runtime": "6.26.0",
    "flow-bin": "^0.61.0",
    "flow-copy-source": "^1.3.0",
    "lodash": "^4.17.5",
    "npmignore": "^0.2.0",
    "simpl-schema": "^1.4.2"
  },
  "scripts": {
    "test": "ava",
    "prepublish": "npm run prepublish-babel && npm run prepublish-copy-flow-info && npmignore -u lib",
    "prepublish-babel": "NODE_ENV=production babel --no-babelrc src/ -d lib --presets=es2015,react-app --plugins=transform-flow-strip-types --ignore '*.test.js'",
    "prepublish-copy-flow-info": "flow-copy-source src lib",
    "flow": "flow-bin"
  },
  "ava": {
    "require": [
      "babel-register"
    ],
    "babel": "inherit"
  },
  "babel": {
    "presets": [
      "@ava/stage-4"
    ],
    "plugins": [
      "transform-class-properties",
      "transform-flow-strip-types"
    ]
  }
}
